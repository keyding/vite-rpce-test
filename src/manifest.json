{
  "manifest_version": 3,
  "action": { "default_popup": "index.html" },
  "icons": {
    "16": "icons/16x16.png",
    "48": "icons/48x48.png",
    "128": "icons/128x128.png"
  },
  "background": {
    "service_worker": "src/background.js"
  },
  "content_scripts": [
    {
      "js": ["src/content-scripts/index.js"],
      "matches": ["https://www.google.com/*"]
    }
  ]
}
